{
  "tabWidth": 2,
  "printWidth": 100,
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "css",
  "proseWrap": "always",
  "quoteProps": "as-needed",
  "trailingComma": "none",
  "bracketSpacing": true,
  "insertPragma": false,
  "bracketSameLine": true,
  "jsxSingleQuote": false,
  "requirePragma": false,
  "semi": true,
  "singleQuote": false,
  "useTabs": false
}
